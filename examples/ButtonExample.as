package{	import flash.display.MovieClip;		import com.rafaelrinaldi.ui.button.Button;		public class ButtonExample extends MovieClip	{		public var noHit, hit, explode : Button;				public function ButtonExample()		{			noHit = new Button(noHitButton);			noHit.onClick = clickHandler;						hit = new Button(hitButton);			hit.onClick = clickHandler;						explode = new Button(explodeButton);			explode.onClick = clickHandler;		}				protected function clickHandler( p_button : Button ) : void		{			if(p_button.view == noHitButton) {				trace("No hit button!");			} else if(p_button.view == hitButton) {				trace("Hit button!");			} else if(p_button.view == explodeButton) {				trace("Explode button!");			}		}	}}